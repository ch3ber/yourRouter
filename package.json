{
  "name": "yourrouter",
  "version": "1.1.2",
  "description": "A powerful router based on client-side routing.",
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./utils/*": "./lib/utils/*.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "sh ./pre-build.sh && tsc --project tsconfig.production.json && tsc-alias",
    "build:watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "dev": "tsc --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "npm run build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:vite": "vite",
    "test:watch": "vitest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ch3ber/yourRouter.git"
  },
  "keywords": [
    "javascript",
    "vanilla-router",
    "frontend-router",
    "client-side",
    "router",
    "router.js",
    "yourrouter"
  ],
  "author": "Eber Edrey Alejo Berrones <eber4202@hotmail.com> (https://ch3ber.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ch3ber/yourRouter/issues"
  },
  "homepage": "https://ch3ber.github.io/yourRouter",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "concurrently": "7.3.0",
    "eslint": "8.22.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.5",
    "eslint-plugin-promise": "6.0.0",
    "tsc-alias": "1.7.0",
    "typescript": "4.7.4",
    "vite": "3.0.9",
    "vitest": "0.33.0"
  }
}
