{
  "name": "yourrouter",
  "version": "1.1.0",
  "description": "A powerful router based on client-side routing.",
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./utils/*": "./lib/utils/*.js"
  },
  "directories": {
    "doc": "doc",
    "example": "examples",
    "lib": "lib"
  },
  "scripts": {
    "dev": "vite",
    "start": "tsc --watch",
    "prepare": "npm run build",
    "build": "tsc && tsc-alias",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ch3ber/yourRouter.git"
  },
  "keywords": [
    "javascript",
    "vanilla-router",
    "frontend-router",
    "client-side",
    "router",
    "router.js",
    "yourrouter"
  ],
  "author": "Eber Edrey Alejo Berrones <eber4202@hotmail.com> (https://ch3ber.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ch3ber/yourRouter/issues"
  },
  "homepage": "https://ch3ber.github.io/yourRouter",
  "dependencies": {
    "typescript": "4.7.4",
    "vite": "3.0.9"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "eslint": "8.22.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.5",
    "eslint-plugin-promise": "6.0.0",
    "tsc-alias": "1.7.0"
  }
}
